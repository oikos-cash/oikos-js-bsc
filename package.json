{
  "name": "@oikos/oikos-js-bsc",
  "version": "0.2.32",
  "description": "Oikos JS Library - access Oikos smart contracts from browser and Node.js",
  "main": "dist/main.node.js",
  "browser": "dist/main.js",
  "homepage": "https://docs.oikos.cash",
  "repository": "https://github.com/oikos-cash/oikos-js-bsc",
  "scripts": {
    "test": "jest --runInBand --verbose",
    "clean-install": "rm ./package-lock.json && rm -rf node_modules/* && npm install",
    "clear": "$(rm dist/*.js || true) && $(rm src/contracts/*.js || true) && $(rm lib/abis/*.js || true) && $(rm lib/addresses.js || true)",
    "lint": "eslint .",
    "docs": "jsdoc src/index*.js src/contracts/bsc src/util -r -d ./doc -c jsdoc.json --readme README.md",
    "format": "prettier --write '{tools,lib,src,__tests__}/**/*.js'",
    "abitojs": "node ./tools/abitojs.js",
    "webpack": "webpack --mode=production",
    "build": "npm run clear && npm run abitojs && npm run format && npm run docs && npm run webpack && npm test",
    "rates": "babel-node examples/rates.js",
    "repl": "NODE_OPTIONS='--experimental-repl-await' node ./repl",
    "repl:shasta": "TRON_NETWORK=shasta npm run repl",
    "repl:mainnet": "TRON_NETWORK=mainnet npm run repl"
  },
  "author": "Oikos",
  "license": "MIT",
  "dependencies": {
    "@babel/node": "^7.12.17",
    "@ledgerhq/hw-app-eth": "4.74.2",
    "@ledgerhq/hw-transport": "4.74.2",
    "@ledgerhq/hw-transport-u2f": "4.74.2",
    "@oikos/oikos-bsc": "^0.2.32",
    "@oikos/oikos-data-bsc": "^0.1.8",
    "@walletconnect/web3-provider": "1.0.0-beta.47",
    "bignumber.js": "^9.0.1",
    "ethereumjs-tx": "1.3.7",
    "ethereumjs-util": "5.2.0",
    "ethers": "5.0.31",
    "hdkey": "1.1.1",
    "lodash": "4.17.15",
    "trezor-connect": "7.0.3",
    "walletlink": "1.0.0",
    "web3-providers-ws": "^1.3.4",
    "web3-utils": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-transform-runtime": "~7.0.0",
    "@babel/preset-env": "~7.0.0",
    "@babel/runtime": "~7.0.0",
    "axios": "~0.18.0",
    "babel-core": "~7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "24.8.0",
    "babel-loader": "~8.0.2",
    "cheerio": "~1.0.0-rc.2",
    "docdash": "~1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-last": "0.0.5",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "24.8.0",
    "jsdoc": "3.6.3",
    "prettier": "~1.14.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  }
}
